	<queryString>
		<![CDATA[SELECT SUM(RES_INI_ES) RES_INI_ES, SUM(TOT_ACC_IMP) TOT_ACC_IMP, SUM(RIS_PAG) RIS_PAG, SUM(RES_INI_ES_PREC) RES_INI_ES_PREC, SUM(TOT_ACC_IMP_PREC) TOT_ACC_IMP_PREC, SUM(RIS_PAG_PREC) RIS_PAG_PREC, 
SUM(FONDO_INIZ_CASSA_N) FONDO_INIZ_CASSA_N, SUM(FONDO_INIZ_CASSA_PREC_N) FONDO_INIZ_CASSA_PREC_N, SUM(AVANZO_N) AVANZO_N, SUM(AVANZO_PREC_N) AVANZO_PREC_N, SUM(DISAVANZO_N) DISAVANZO_N, SUM(DISAVANZO_PREC_N) DISAVANZO_PREC_N
FROM (
Select
0 RES_INI_ES,
SUM(DECODE(TIPO,'S',TOT_ACC_IMP,-TOT_ACC_IMP)) TOT_ACC_IMP,
SUM(DECODE(TIPO,'S',Nvl(RIS_PAG,0)+Nvl(RES_RIS_PAG,0),-(Nvl(RIS_PAG,0)+Nvl(RES_RIS_PAG,0)))) RIS_PAG,
0 RES_INI_ES_PREC,
SUM(DECODE(TIPO,'S',TOT_ACC_IMP_PREC,-TOT_ACC_IMP_PREC)) TOT_ACC_IMP_PREC,
SUM(DECODE(TIPO,'S',Nvl(RIS_PAG_PREC,0),-(Nvl(RIS_PAG_PREC,0)))) RIS_PAG_PREC,
0 FONDO_INIZ_CASSA_N, 0 FONDO_INIZ_CASSA_PREC_N, 0 AVANZO_N, 0 AVANZO_PREC_N, 0 DISAVANZO_N, 0 DISAVANZO_PREC_N
FROM TMP_STAMPA_RENDICONTO
UNION ALL
SELECT 0 RES_INI_ES, 0 TOT_ACC_IMP, 0 RIS_PAG, 0 RES_INI_ES_PREC, 0 TOT_ACC_IMP_PREC, 0 RIS_PAG_PREC,
NVL(SUM(FONDO_CASSA), 0) FONDO_INIZ_CASSA_N,
NVL(SUM(FONDO_CASSA_PRE),0) FONDO_INIZ_CASSA_PREC_N,
NVL(SUM(CASE WHEN NVL(AVANZO,0)>0
             THEN AVANZO
             ELSE 0
             END), 0) AVANZO_N,
NVL(SUM(CASE WHEN NVL(AVANZO_PRE,0)>0
             THEN AVANZO_PRE
             ELSE 0
             END), 0) AVANZO_PREC_N,
NVL(SUM(CASE WHEN NVL(AVANZO,0)<0
             THEN ABS(AVANZO)
             ELSE 0
             END), 0) DISAVANZO_N,
NVL(SUM(CASE WHEN NVL(AVANZO_PRE,0)<0
             THEN ABS(AVANZO_PRE)
             ELSE 0
             END), 0) DISAVANZO_PREC_N
FROM (SELECT IM01 FONDO_CASSA, 0 FONDO_CASSA_PRE, 0 AVANZO, 0 AVANZO_PRE
      FROM CONFIGURAZIONE_CNR
      WHERE ESERCIZIO=$P{P_ESERCIZIO}
      AND   CD_CHIAVE_PRIMARIA = 'BILANCIO_ENTE'
      AND   CD_CHIAVE_SECONDARIA = 'FONDO_CASSA_DEF'
      UNION ALL
      SELECT 0 FONDO_CASSA, IM01 FONDO_CASSA_PRE, 0 AVANZO, 0 AVANZO_PRE
      FROM CONFIGURAZIONE_CNR
      WHERE ESERCIZIO=$P{P_ESERCIZIO}-1
      AND   CD_CHIAVE_PRIMARIA = 'BILANCIO_ENTE'
      AND   CD_CHIAVE_SECONDARIA = 'FONDO_CASSA_DEF'
      UNION ALL
      SELECT 0 FONDO_CASSA, 0 FONDO_CASSA_PRE, IM01 AVANZO, 0 AVANZO_PRE
      FROM CONFIGURAZIONE_CNR
      WHERE ESERCIZIO=$P{P_ESERCIZIO}
      AND   CD_CHIAVE_PRIMARIA = 'BILANCIO_ENTE'
      AND   CD_CHIAVE_SECONDARIA = 'AVANZO_DEF'
      UNION ALL
      SELECT 0 FONDO_CASSA, 0 FONDO_CASSA_PRE, 0 AVANZO, IM01 AVANZO_PRE
      FROM CONFIGURAZIONE_CNR
      WHERE ESERCIZIO=$P{P_ESERCIZIO}-1
      AND   CD_CHIAVE_PRIMARIA = 'BILANCIO_ENTE'
      AND   CD_CHIAVE_SECONDARIA = 'AVANZO_DEF'))]]>
	</queryString>
