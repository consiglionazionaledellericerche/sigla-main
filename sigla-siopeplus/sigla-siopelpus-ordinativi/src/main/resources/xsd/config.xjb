               xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
               xmlns:simplify="http://jaxb2-commons.dev.java.net/basic/simplify" extensionBindingPrefixes="simplify"
               jaxb:extensionBindingPrefixes="xjc"
               version="2.0">
        <jaxb:bindings schemaLocation="OPI_MSG_RICEZIONE_FLUSSO_V_1_3_1a.xsd" >
