			name="titolo" />
		<formFieldProperty
			name="file" />
		<formFieldProperty
			name="attivaFile" />	    	    
	</form>	
	
	<columnSet name="decorrenzaTermini">
		<columnFieldProperty
			name="aspectName"
			keysProperty="aspectNamesDecorrenzaTerminiKeys" />
		<columnFieldProperty
			name="nome" />
		<columnFieldProperty
			name="descrizione" />
		<columnFieldProperty
			name="titolo" />    	    
	</columnSet>	
	
	<columnSet name="default">
		<columnFieldProperty
			name="aspectName"/>
		<columnFieldProperty
			name="nome" />
		<columnFieldProperty
			name="descrizione" />
		<columnFieldProperty
			name="titolo" />
	</columnSet>	
	<columnSet name="fatturaAttiva">
		<columnFieldProperty
			name="nome" />
		<columnFieldProperty
			name="descrizione" />
		<columnFieldProperty
			name="titolo" />
