<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="aspectName"
		property="aspectName"
		inputType="SELECT"
		keysProperty="aspectNamesKeys"
		inputSize="40"
		nullable="false"
		label="Tipologia" />
	<fieldProperty
		name="utente"
		inputType="TEXT"
		property="utenteSIGLA"
		label="Utente"/>
	<fieldProperty
		name="data"
		inputType="TEXT"
		property="lastModificationDate"
		formatName="timestamp"
		label="Data"/>

	<form name="readonly">
		<formFieldProperty
				enabledOnEdit="false"
				name="aspectName" />
		<formFieldProperty
				name="nome" />
		<formFieldProperty
				name="descrizione"
				enabledOnEdit="false" />
		<formFieldProperty
				name="titolo"
				enabledOnEdit="false" />
		<formFieldProperty
				name="attivaFile" />
	</form>

	<form name="default">
		<formFieldProperty
			name="aspectName" />
		<formFieldProperty
			name="nome" />
		<formFieldProperty
			name="descrizione" />
		<formFieldProperty
			name="titolo" />
		<formFieldProperty
			name="file" />
		<formFieldProperty
			name="attivaFile" />	    	    
	</form>	
	
	<form name="decorrenzaTermini">
		<formFieldProperty
			name="aspectName"
			keysProperty="aspectNamesDecorrenzaTerminiKeys" />
		<formFieldProperty
			name="nome" />
		<formFieldProperty
			name="descrizione" />
		<formFieldProperty
			name="titolo" />
		<formFieldProperty
			name="file" />
		<formFieldProperty
			name="attivaFile" />	    	    
	</form>	
			
	<form name="fatturaAttiva">
		<formFieldProperty
			name="nome" />
		<formFieldProperty
			name="descrizione" />
		<formFieldProperty
			name="titolo" />
		<formFieldProperty
			name="file" />
		<formFieldProperty
			name="attivaFile" />	    	    
	</form>	
	
	<columnSet name="decorrenzaTermini">
		<columnFieldProperty
			name="aspectName"
			keysProperty="aspectNamesDecorrenzaTerminiKeys" />
		<columnFieldProperty
			name="nome" />
		<columnFieldProperty
			name="descrizione" />
		<columnFieldProperty
			name="titolo" />
		<columnFieldProperty
			name="utente"/>
		<columnFieldProperty
			name="data"/>
	</columnSet>	
	
	<columnSet name="default">
		<columnFieldProperty
			name="aspectName"/>
		<columnFieldProperty
			name="nome" />
		<columnFieldProperty
			name="descrizione" />
		<columnFieldProperty
			name="titolo" />
		<columnFieldProperty
			name="utente"/>
		<columnFieldProperty
			name="data"/>
	</columnSet>

	<columnSet name="readonly">
		<columnFieldProperty
				name="aspectName"/>
		<columnFieldProperty
				name="nome" />
		<columnFieldProperty
				name="descrizione" />
		<columnFieldProperty
				name="titolo" />
		<columnFieldProperty
				name="utente"/>
		<columnFieldProperty
				name="data"/>
	</columnSet>

	<columnSet name="fatturaAttiva">
		<columnFieldProperty
			name="nome" />
		<columnFieldProperty
			name="descrizione" />
		<columnFieldProperty
			name="titolo" />
	</columnSet>	
</bulkInfo>
