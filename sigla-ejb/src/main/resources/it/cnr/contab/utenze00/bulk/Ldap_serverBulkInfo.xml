<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="hostname"
		property="hostname"
		inputType="TEXTAREA"
		cols="60"
		rows="5"
		maxLength="2000"
		label="Hostname del server" />
	<fieldProperty
		name="port"
		property="port"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="port" />
	<fieldProperty
		name="fl_attivo"
		property="fl_attivo"
		inputType="CHECKBOX"
		inputSize="1"
		maxLength="1"
		label="Server attivo." />
	<fieldProperty
		name="fl_master"
		property="fl_master"
		inputType="CHECKBOX"
		inputSize="1"
		maxLength="1"
		label="Server master, abilitato in scrittura." />
	<fieldProperty
		name="livello_priorita"
		property="livello_priorita"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Livello di priorita' per la lettura dei dati, valore basso indica priorita' maggiore" />
	<fieldProperty
		name="fl_ssl"
		property="fl_ssl"
		inputType="CHECKBOX"
		inputSize="1"
		maxLength="1"
		label="Tipo di connessione SSL" />
	<formFieldProperty
		name="hostname" />
	<formFieldProperty
		name="port" />
	<formFieldProperty
		name="fl_attivo" />
	<formFieldProperty
		name="fl_master" />
	<formFieldProperty
		name="livello_priorita" />
	<formFieldProperty
		name="fl_ssl" />
	<findFieldProperty
		name="hostname" />
	<findFieldProperty
		name="port" />
	<findFieldProperty
		name="fl_attivo" />
	<findFieldProperty
		name="fl_master" />
	<findFieldProperty
		name="livello_priorita" />
	<findFieldProperty
		name="fl_ssl" />
	<columnFieldProperty
		name="hostname" />
	<columnFieldProperty
		name="port" />
	<columnFieldProperty
		name="fl_attivo" />
	<columnFieldProperty
		name="fl_master" />
	<columnFieldProperty
		name="livello_priorita" />
	<columnFieldProperty
		name="fl_ssl" />
</bulkInfo>