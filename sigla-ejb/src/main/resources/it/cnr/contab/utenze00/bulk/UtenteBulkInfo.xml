		name="fl_autenticazione_ldap"
		property="fl_autenticazione_ldap"
		inputType="CHECKBOX"
		inputSize="1"
		maxLength="1"
		enabledOnEdit="false"
		enabledOnInsert="true"
		readonlyProperty="ROLdap"
		label="Autenticazione su server LDAP" />
	<fieldProperty
		name="cd_utente_uid"
		property="cd_utente_uid"
