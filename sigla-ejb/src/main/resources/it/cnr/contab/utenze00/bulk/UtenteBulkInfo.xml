			name="cd_utente" />
		<columnFieldProperty
			name="cd_utente_uid" />
	</columnSet>

	<columnSet
			name="restService">
		<columnFieldProperty
				name="cd_utente" />
	</columnSet>

	<findFieldProperty
		name="cd_utente" />
	<findFieldProperty
		name="cd_utente_uid"
		label="Codice Utente Ufficiale CNR (nome.cognome)" />
	
