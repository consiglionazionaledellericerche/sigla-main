		name="ruolo_supervisore"
		property="ruolo_supervisore"
		inputType="SEARCHTOOL"
		formName="searchtool"
		enabledOnSearch="true"
		label="Ruolo Supervisore" />
	<fieldProperty
		name="fl_altra_proc"
		property="fl_altra_proc"
		inputType="CHECKBOX"
		inputSize="1"
		maxLength="1"
		enabledOnEdit="false"
		enabledOnInsert="false"
		label="Utente gestito da procedure esterne" />
	<fieldProperty
		name="fl_attiva_blocco"
		property="fl_attiva_blocco"
		inputType="CHECKBOX"
		inputSize="1"
		maxLength="1"
		enabledOnEdit="false"
		enabledOnInsert="false"
		label="Utente soggetto a blocco delle funzioni" />

