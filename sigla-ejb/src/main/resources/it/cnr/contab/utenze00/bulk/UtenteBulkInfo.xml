		name="cd_cds_configuratore"
		property="cd_cds_configuratore"
		inputType="TEXT"
		inputSize="20"
		maxLength="30"
		label="Codice CDS amministratore" />
	<fieldProperty
		name="cd_gestore"
		property="cd_gestore"
		inputType="TEXT"
		inputSize="10"
		maxLength="20"
		label="Gestore" />
