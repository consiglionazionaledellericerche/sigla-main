<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd" shortDescription="Ripartizione costi" longDescription="Ripartizione costi">
	
	<fieldProperty 
		name="esercizio" 
		property="esercizio" 
		inputType="ROTEXT" 
		inputSize="4" 
		maxLength="4" 
		label="Esercizio"/>
	<fieldProperty 
		name="mese" 
		property="mese" 
		inputType="SELECT" 
		label="Mese" 
		keysProperty="meseKeys"
		nullable="false"/>
	<fieldProperty 
		name="cds" 
		property="cds" 
		inputType="ROTEXT" 
		inputSize="10" 
		maxLength="10" 
		label="CDS"/>
	

	 <fieldProperty
		name="find_uo"
		property="v_uo"
		inputType="SEARCHTOOL"
		formName="searchtoolUO"
		enabledOnSearch="true"
		enabledOnEdit="true"
		readonlyProperty="ROFindV_uo"
		label="UnitÃ  organizzativa"/>
	<fieldProperty
		name="cd_uo"
		property="v_uo.cd_unita_organizzativa"
		readonlyProperty="ROFindV_uo"/>	
	<fieldProperty
		name="ds_uo"
		property="v_uo.ds_unita_organizzativa"/>
		
	<fieldProperty
		name="find_commessa"
		property="v_commessa"
		inputType="SEARCHTOOL"
		formName="searchtool_commessa"
		enabledOnSearch="true"
		completeOnSave="true"
		columnSet="filtro_ricerca_commesse"
		freeSearchSet="filtro_ricerca_commesse"
