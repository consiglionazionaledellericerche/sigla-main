		name="find_terzo" />
	<formFieldProperty
		name="ds_terzo" />

	<columnFieldProperty
	    headerLabel="Dati Associato"
	    label="Codice Terzo"
		name="cd_terzo" />
	<columnFieldProperty
	    headerLabel="Dati Associato"
	    label="Nominativo"
		name="ds_terzo" />
	<columnFieldProperty
		name="dt_canc" />

	<findFieldProperty
		name="cd_terzo" />
	<findFieldProperty
		name="dt_canc" />
</bulkInfo>