		formatName="date_short"
		nullable="false"/>	
	<fieldProperty
		name="dt_inizio_val_dich"
		property="dt_inizio_val_dich"
		label="Data di inizio periodo di riferimento"
		readonlyProperty="RODichiarazione"
		inputType="TEXT"
		formatName="date_short"/>	
	<fieldProperty
		name="dt_fine_val_dich"
		property="dt_fine_val_dich"
		label="Data di fine periodo di riferimento"
		readonlyProperty="RODichiarazione"
		inputType="TEXT"
		formatName="date_short"/>
	<fieldProperty
		name="im_limite_sing_op"
		property="im_limite_sing_op"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		readonlyProperty="RODichiarazione"
		enabledOnSearch="true"		
		label="Importo limite singola operazione"/>
	<fieldProperty
		name="im_limite_op"
		property="im_limite_op"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		readonlyProperty="RODichiarazione"
		enabledOnSearch="true"		
		label="Importo limite totale"/>					
	<fieldProperty
		name="id_dichiarazione"
		property="id_dichiarazione"
		inputType="TEXT"
		inputSize="17"
		maxLength="17"
		caseSensitiveSearch="false" 
		readonlyProperty="RODichiarazione"
		enabledOnSearch="true"		
		label="codice telematico"
		nullable="false"/>		
		
