<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<fieldProperty
		name="anagrafico"
		property="anagrafico"
		label="anagrafico"
		inputType="TEXT"/>
	<fieldProperty
		name="esercizio"
		property="esercizio"
		label="Esercizio"
		inputType="ROTEXT"/>	
	<fieldProperty
		name="progr"
		property="progr"
		label="Numero prestatore"
		inputType="ROTEXT"/>		
	<fieldProperty
		name="anno_dich"
		property="anno_dich"
		label="Esercizio dichiarante"
		inputType="TEXT"
		readonlyProperty="RODichiarazione"
		nullable="false"/>
	<fieldProperty
		name="num_dich"
		property="num_dich"
