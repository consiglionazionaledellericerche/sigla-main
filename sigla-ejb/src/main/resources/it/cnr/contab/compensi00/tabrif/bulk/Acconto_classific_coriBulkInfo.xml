<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio" />
	<fieldProperty
		name="cd_anag"
		property="anagrafico.cd_anag"
		inputType="TEXT"
		inputSize="8"
		maxLength="8"
		label="Codice Anagrafico" />
	<fieldProperty
		name="descrizioneAnagrafica"
		property="anagrafico.descrizioneAnagrafica"
		label="Denominazione"
		inputSize="100"
		enabledOnSearch="true"
		inputType="TEXT" />			
	<fieldProperty
		name="cd_classificazione_cori"
		property="cd_classificazione_cori"
		inputType="TEXT"
		inputSize="2"
		maxLength="2"
		label="Codice classificazione tipo contributo ritenuta" />
	<fieldProperty
		name="imponibile"
		property="imponibile"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		label="Imponibile" />
	<fieldProperty
		name="aliquota"
		property="aliquota"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		label="Aliquota" />
	<fieldProperty
		name="im_acconto_calcolato"
		property="im_acconto_calcolato"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		label="Acconto calcolato" />
	<fieldProperty
		name="im_acconto_trattenuto"
		property="im_acconto_trattenuto"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		label="Acconto trattenuto" />
	<fieldProperty
		name="pg_comune"
		property="comune.pg_comune"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		label="Progressivo Comune" />
	<fieldProperty
		name="cd_catastale"
		property="comune.cd_catastale"
		label="Codice Catastale"
		inputSize="10"
		enabledOnSearch="true"
		inputType="TEXT" />		
	<fieldProperty
		name="ds_comune"
		property="comune.ds_comune"
		label="Descrizione Comune"
		inputSize="100"
		enabledOnSearch="true"
		inputType="TEXT" />			
	<fieldProperty
		name="cd_cds_conguaglio"
		property="cd_cds_conguaglio"
		inputType="TEXT"
		inputSize="30"
		maxLength="30"
		label="CDS Conguaglio" />		
	<fieldProperty
		name="cd_uo_conguaglio"
		property="cd_uo_conguaglio"
		inputType="TEXT"
		inputSize="30"
		maxLength="30"
		label="UO Conguaglio" />		
