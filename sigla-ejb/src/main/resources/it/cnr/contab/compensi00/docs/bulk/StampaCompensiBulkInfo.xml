	<fieldProperty
		name="pgInizio"
		property="pgInizio"
		inputType="TEXT"
		maxLength="10"
		inputSize="10"
		nullable="false"
		label="Numero Inizio" />
	<fieldProperty
		name="pgFine"
		property="pgFine"
