<SQLPersistentInfo
	persistentClassName="it.cnr.contab.compensi00.tabrif.bulk.ScaglioneBulk"
	homeClassName="it.cnr.contab.compensi00.tabrif.bulk.ScaglioneHome" >
	<defaultColumnMap
		tableName="SCAGLIONE" >

		<columnMapping
			columnName="CD_PROVINCIA"
			propertyName="provincia.cd_provincia" />
		<columnMapping
			columnName="CD_REGIONE"
			propertyName="regione.cd_regione" />
		<columnMapping
			columnName="PG_COMUNE"
			propertyName="comune.pg_comune" />
		<columnMapping
			columnName="CD_CONTRIBUTO_RITENUTA"
			propertyName="contributo_ritenuta.cd_contributo_ritenuta" />								
	</defaultColumnMap>
	<columnMap name="COMUNE"
			   tableName="SCAGLIONE"
			   extends="default">
		<columnMapping
				columnName="COMUNE.CD_CATASTALE"
				propertyName="comune.cd_catastale"
				sqlTypeName="VARCHAR"
				columnSize="10"
				nullable="false" />
		<columnMapping
				columnName="COMUNE.CD_PROVINCIA"
				propertyName="comune.cd_provincia"
				sqlTypeName="VARCHAR"
				columnSize="10" />
		<columnMapping
				columnName="COMUNE.DS_COMUNE"
				propertyName="comune.ds_comune"
				sqlTypeName="VARCHAR"
				columnSize="100" />
	</columnMap>
	<persistentProperty
		name="cd_contributo_ritenuta"
		partOfOid="true" />
	<persistentProperty
		name="provincia"
		partOfOid="true" />		
	<persistentProperty
		name="regione"
		partOfOid="true" />
	<persistentProperty
		name="comune"
		partOfOid="true" />				
		
</SQLPersistentInfo>