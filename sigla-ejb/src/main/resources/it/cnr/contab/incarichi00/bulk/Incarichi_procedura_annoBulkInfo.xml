<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="incarichi_procedura.esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		nullable="false"
		label="Anno" />
	<fieldProperty
		name="pg_procedura"
		property="incarichi_procedura.pg_procedura"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		nullable="false"
		label="Identificativo Ente" />
	<fieldProperty
		name="esercizio_limite"
		property="esercizio_limite"
		optionsProperty="anniList"
		readonlyPropertyOnEdit="ROEsercizio_limite"
		inputType="SELECT"
		inputSize="4"
		maxLength="4"
		enabledOnSearch="true"
		nullable="false"
		label="Esercizio" />
	<fieldProperty
		name="importo_iniziale"
		property="importo_iniziale"
		readonlyPropertyOnEdit="ROImporto_iniziale"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		enabledOnSearch="true"
		nullable="false"
		label="Importo" />
	<fieldProperty
		name="importo_complessivo"
		property="importo_complessivo"
		readonlyPropertyOnEdit="ROImporto_complessivo"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		enabledOnSearch="true"
		nullable="false"
		label="Importo effettivo" />
	<fieldProperty
		name="importo_ripartito"
		property="importo_ripartito"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		label="Importo ripartito" />
	<fieldProperty
		name="importo_utilizzato"
		property="importo_utilizzato"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		nullable="false"
		label="Importo utilizzato" />

  	<form name="searchtool">
          <formFieldProperty
                name="esercizio"/>
          <formFieldProperty
                name="pg_procedura"/>
	   	  <formFieldProperty
				name="esercizio_limite" 
				inputType="TEXT" />                
    </form>

	<formFieldProperty
		name="esercizio" />
	<formFieldProperty
		name="pg_procedura" />
	<formFieldProperty
		name="esercizio_limite" />
	<formFieldProperty
		name="importo_iniziale" />
	<formFieldProperty
		name="importo_complessivo" />

	<columnFieldProperty
		name="esercizio_limite" />
	<columnFieldProperty
		name="importo_iniziale"
		label="importo"
		headerLabel="IMPORTO" />
	<columnFieldProperty
		name="importo_complessivo"
		label="effettivo"
		headerLabel="IMPORTO" />
	<columnFieldProperty
		name="importo_utilizzato"
		label="utilizzato"
		headerLabel="IMPORTO" />

	<columnSet name="multi_incarico">
		<columnFieldProperty
			name="esercizio_limite" />
		<columnFieldProperty
			name="importo_iniziale"
			label="importo"
			headerLabel="IMPORTO" />
		<columnFieldProperty
			name="importo_complessivo"
			label="effettivo"
			headerLabel="IMPORTO" />
		<columnFieldProperty
			name="importo_ripartito"
			label="ripartito&lt;BR&gt;su contratti"
			headerLabel="IMPORTO" />
		<columnFieldProperty
			name="importo_utilizzato"
			label="utilizzato&lt;BR&gt;su compensi"
			headerLabel="IMPORTO" />
	</columnSet>

	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="pg_procedura" />
	<findFieldProperty
		name="esercizio_limite" />
	<findFieldProperty
		name="importo_iniziale" />
	<findFieldProperty
		name="importo_complessivo" />
</bulkInfo>
