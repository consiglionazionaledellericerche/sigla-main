<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="findUbicazioneDestinazione"
		property="ubicazione_destinazione"
		inputType="SEARCHTOOL"
		formName="searchtool"
		completeOnSave = "true"
		label="Ubicazione Destinazione" />
	<fieldProperty
		name="cdUbicazioneDestinazione"
		property="ubicazione_destinazione.cd_ubicazione"
		maxLength="30"
		inputSize="20"
		inputType="TEXT"
		nullable="false"
		readonlyProperty="ROUbicazioneDestinazione"
		label="Codice Ubicazione" />
	/*****************************************************	
	* Riferimento all'oggetto complesso assegnatario **/
	<fieldProperty
		name="find_assegnatario"
		property="assegnatario"
		inputType="SEARCHTOOL"
		formName="searchtool"
		completeOnSave = "true"
		label="Assegnatario"  />
	<fieldProperty
		name="cd_assegnatario"
		property="assegnatario.cd_terzo"
		maxLength="10"
		inputSize="10"
		inputType="TEXT"
		label="Assegnatario"	
		readonlyProperty="ROassegnatarioBene"
		caseSensitiveSearch="false"
		nullable="false" />
	<fieldProperty
		name="stato"
		property="stato"
		inputType="SELECT"
		inputSize="10"
		maxLength="3"
		keysProperty="statoKeys"
		label="Stato" />
	<fieldProperty
		name="blob"
		property="blob"
		inputType="FILE"
		inputSize="40"
		maxLength="400"
		caseSensitiveSearch="false"
		label="Allegato" />

	<!--  commento -->
	<formFieldProperty
		name="find_assegnatario" />
	<formFieldProperty
		name="cd_assegnatario" />
	<formFieldProperty 
		name="findUbicazioneDestinazione" />
	<formFieldProperty 
		name="cdUbicazioneDestinazione" />
	<formFieldProperty
		name="stato" />
	<formFieldProperty
		name="blob" />
</bulkInfo>