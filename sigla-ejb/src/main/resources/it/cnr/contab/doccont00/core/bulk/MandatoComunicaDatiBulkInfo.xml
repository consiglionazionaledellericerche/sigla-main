<bulkInfo
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://contab.cnr.it/schema/SIGLA"
        xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd"
        shortDescription="Mandato">
	<fieldProperty
			name="esercizio"
			property="esercizio"
			inputType="TEXT"
			label="Esercizio"
			maxLength="4"
			inputSize="8"
			enabledOnSearch="true"
			enabledOnFreeSearch="true"/>
	<fieldProperty
			name="cd_cds"
			property="cd_cds"
			inputType="TEXT"
			maxLength="30"
			inputSize="8"
			label="Cds"
			enabledOnSearch="true"
			enabledOnFreeSearch="true" />
	<fieldProperty
			name="pg_mandato"
			property="pg_mandato"
			inputType="TEXT"
			label="Numero Mandato"
			maxLength="10"
			inputSize="8"
			enabledOnEdit="false"
			enabledOnInsert="false"
			enabledOnSearch="true"/>
    <fieldProperty
            name="denominazioneSede"
            property="denominazioneSede"
            inputSize="500"
            inputType="TEXT"
            label="Denominazione"/>
    <fieldProperty
            name="importo"
            property="importo"
            inputType="TEXT"
            label="Importo per Capitolo"/>
    <fieldProperty
            name="cdLiv4"
            property="cdLiv4"
            inputType="TEXT"
            label="Codice"/>
    <fieldProperty
            name="dsLiv4"
            property="dsLiv4"
            inputType="TEXT"
            label="Descrizione"/>

    <formFieldProperty
            name="denominazioneSede"/>
    <formFieldProperty
            name="importo"/>
    <formFieldProperty
            name="cdLiv4"/>
    <formFieldProperty
            name="dsLiv4"/>

	<findFieldProperty
			name="esercizio"/>
	<findFieldProperty
			name="cd_cds"/>
	<findFieldProperty
			name="pg_mandato"/>

</bulkInfo>