<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="cd_cds"
		property="cds.cd_unita_organizzativa"
		inputType="TEXT"
		inputSize="30"
		maxLength="30"/>
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio" />
	<fieldProperty
		name="pg_modifica"
		property="pg_modifica"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		enabledOnEdit="false"
		enabledOnInsert="false"
		label="Numero Modifica" />
	<fieldProperty
		name="ds_modifica"
		property="ds_modifica"
		inputType="TEXTAREA"
		cols="60"
		rows="5"
		maxLength="300"
		label="Descrizione"
		nullable="false" />
	<fieldProperty
		name="dt_modifica"
		property="dt_modifica"
		inputType="TEXT"
		formatName="date_short"
		enabledOnEdit="false"
		enabledOnInsert="false"
		label="Data Modifica" />
	<fieldProperty
		name="pg_accertamento"
		property="pg_accertamento"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		enabledOnEdit="false"
		enabledOnInsert="false"
		label="Num. Accertamento" />
	<fieldProperty
		name="pg_variazione"
		property="variazione.pg_variazione"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		enabledOnEdit="false"
		enabledOnInsert="false"
		label="Num. Variazione"
		nullable="false" />
	<fieldProperty
		name="esercizio_originale"
		property="esercizio_originale"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		enabledOnEdit="false"
		enabledOnInsert="false"
		label="Es. Accertamento" />
	<fieldProperty
		name="variazioni"
		inputType="BUTTON"
		img="img/find16.gif"
		label="Variazione"
		enabledOnView="true"
		readonlyProperty="ROVariazioni"
		readonlyPropertyOnView="ROVariazioni"
		href="javascript:submitForm('doVisualizzaVariazione')" 
		/>
	<formFieldProperty
		name="cd_cds" />
	<formFieldProperty
		name="esercizio" />
	<formFieldProperty
		name="pg_modifica" />
	<formFieldProperty
		name="ds_modifica" />
	<formFieldProperty
		name="dt_modifica" />
	<formFieldProperty
		name="pg_accertamento" />
	<formFieldProperty
		name="pg_variazione" />
	<formFieldProperty
		name="esercizio_originale" />
	<formFieldProperty
		name="variazioni" />
	<findFieldProperty
		name="cd_cds" />
	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="pg_modifica" />
	<findFieldProperty
		name="ds_modifica" />
	<findFieldProperty
		name="dt_modifica" />
	<findFieldProperty
		name="pg_accertamento" />
	<findFieldProperty
		name="pg_variazione" />
	<findFieldProperty
		name="esercizio_originale" />
	<columnFieldProperty
		name="cd_cds" />
	<columnFieldProperty
		name="esercizio" />
	<columnFieldProperty
		name="pg_modifica" />
	<columnFieldProperty
		name="ds_modifica" />
	<columnFieldProperty
		name="dt_modifica" />
	<columnFieldProperty
		name="pg_accertamento" />
	<columnFieldProperty
		name="pg_variazione" />
	<columnFieldProperty
		name="esercizio_originale" />
